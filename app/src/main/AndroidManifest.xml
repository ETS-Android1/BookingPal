<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inchub.bookinpal">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logs"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.inchub.bookinpal.PackageDetails"
            android:label="Travel Package"
            android:parentActivityName="com.inchub.bookinpal.MainActivity" />
        <activity
            android:name="com.inchub.bookinpal.Gobots"
            android:label="MTN Customer Bot"
            android:parentActivityName="com.inchub.bookinpal.Gobot" />
        <activity
            android:name="com.inchub.bookinpal.EveryWhereWeGo"
            android:label="House Swap"
            android:parentActivityName="com.inchub.bookinpal.MainActivity">

            <!-- Parent activity meta-data to support API level 4+ -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.inchub.bookinpal.MainActivity" />
        </activity>
        <activity
            android:name="com.inchub.bookinpal.Gobot"
            android:label="Go-Bot"
            android:parentActivityName="com.inchub.bookinpal.MainActivity" /> <!-- Parent activity meta-data to support API level 4+ -->
        <activity
            android:name="com.inchub.bookinpal.MTNBusiness"
            android:label="MTN Business Queries"
            android:parentActivityName="com.inchub.bookinpal.MainActivity" />
        <activity
            android:name="com.inchub.bookinpal.CustomerComplaints"
            android:label="Make Package Booking"
            android:parentActivityName="com.inchub.bookinpal.MainActivity" /> <!-- Parent activity meta-data to support API level 4+ -->
        <activity
            android:name="com.inchub.bookinpal.Fibre"
            android:label="Fibre Queries"
            android:parentActivityName="com.inchub.bookinpal.MainActivity" />
        <activity
            android:name="com.inchub.bookinpal.Prepaid"
            android:label="Holiday Bookings"
            android:parentActivityName="com.inchub.bookinpal.MainActivity" /> <!-- Parent activity meta-data to support API level 4+ -->
        <activity
            android:name="com.inchub.bookinpal.Contracts"
            android:label="Travel Booking"
            android:parentActivityName="com.inchub.bookinpal.MainActivity" />
        <activity android:name="com.inchub.bookinpal.Login" /> <!-- Parent activity meta-data to support API level 4+ -->

        <activity android:name="com.inchub.bookinpal.Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.inchub.bookinpal.MainActivity" /> <!-- Parent activity meta-data to support API level 4+ -->
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.inchub.bookinpal.MainActivity" />
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.inchub.bookinpal.MainActivity" /> <!-- Parent activity meta-data to support API level 4+ -->
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.inchub.bookinpal.MainActivity" />
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.inchub.bookinpal.MainActivity" />
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.inchub.bookinpal.MainActivity" />
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.inchub.bookinpal.MainActivity" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <service
            android:name="com.inchub.bookinpal.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>